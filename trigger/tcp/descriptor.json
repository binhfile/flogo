{
  "name": "flogo-trigger-tcp",
  "type": "flogo:trigger",
  "version": "0.0.0",
  "title": "TCP/SSL/Unix Socket Listener & Endpoint manager",
  "description": "TCP/SSL/Unix Socket Listener & Endpoint manager",
  "homepage": "https://github.com/binhfile/flogo/tree/main/trigger/tcp",
  "settings": [
    {
      "name": "network",
      "type": "string",
      "description": "Network type",
      "allowed": ["tcp", "tcp4", "tcp6", "ssl", "unix"],
      "value": "tcp"
    },
    {
      "name": "host",
      "type": "string",
      "description": "Host IP or name",
      "value": "0.0.0.0"
    },
    {
      "name": "port",
      "type": "string",
      "required": true,
      "description": "Port to listen on",
      "value": "12345"
    },
    {
      "name": "timeout",
      "type": "integer",
      "description": "Read and Write timeout in milliseconds",
      "value": 0
    }
  ],
  "output": [
    {
      "name": "data",
      "type": "bytes",
      "description": "Request data"
    }
  ],
  "reply": [
    {
      "name": "reply",
      "type": "bytes",
      "description": "Reply data"
    }
  ],
  "handler": {
    "settings": [
    ]
  }
}
